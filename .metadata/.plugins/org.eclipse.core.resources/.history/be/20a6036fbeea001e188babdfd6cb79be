package testCase;

import org.openqa.selenium.By;
import org.testng.annotations.Test;

import base.base;



public class Button extends base{

	@Test
	public static void ButtonTest() throws InterruptedException {
		// TODO Auto-generated method stub
		
		Thread.sleep(2000);
		driver.findElement(By.xpath("/html/body/div/div/li[2]/a")).click();
		Thread.sleep(2000);
		driver.findElement(By.xpath("/html/body/div/form/div[1]/div/div/button[1]")).click();
		Thread.sleep(2000);
		driver.findElement(By.className("btn btn-lg btn-success")).click();
		Thread.sleep(2000);
		driver.findElement(By.className("btn btn-lg btn-info")).click();
		Thread.sleep(2000);
		driver.findElement(By.className("btn btn-lg btn-warning")).click();
		Thread.sleep(2000);
		driver.findElement(By.className("btn btn-lg btn-danger")).click();
		Thread.sleep(2000);
		driver.findElement(By.className("btn btn-lg btn-primary dropdown-toggle")).click();
		
		System.out.println("Button files success");
		
	}

}
